class AbdallahAlazzam {
  // Class variables or properties
  String name;
  int age;

  // Constructor
  AbdallahAlazzam(String name, int age) {
    this.name = name;
    this.age = age;
  }

  // Class method
  void introduce() {
    print('My name is $name and I am $age years old.');
  }
}

void main() {
  // Create an instance of the class
  AbdallahAlazzam abdallah = AbdallahAlazzam('Abdallah Alazzam', 26);

  // Call the method
  abdallah.introduce();
}
